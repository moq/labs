<Project>
  <!-- Prevent importing the shared props -->
  <PropertyGroup>
    <RootPath>$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), .gitignore))\</RootPath>
    <BinPath>$(RootPath)bin\$(Configuration)\</BinPath>
    <SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
  </PropertyGroup>
</Project>